{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(Windows MSVC) Debug",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/target/debug/${workspaceFolderBasename}.exe",
			"args": [
				"est"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"console": "integratedTerminal",
			"preLaunchTask": "cargo build"
		},
		{
			"name": "(Windows MinGW) Debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/target/debug/${workspaceFolderBasename}.exe",
			"miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
			"args": [
				"est"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"console": "integratedTerminal",
			"preLaunchTask": "cargo build"
		},
		{
			"name": "(Linux) Debug",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/target/debug/${workspaceFolderBasename}",
			"args": [
				"est"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"console": "integratedTerminal",
			"preLaunchTask": "cargo build"
		},
		{
			"name": "(Linux) Debug Unit Tests",
			"type": "lldb",
			"request": "launch",
			"program": "cargo",
			"args": [
				"test"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"console": "integratedTerminal",
			"preLaunchTask": "cargo build"
		}
	]
}